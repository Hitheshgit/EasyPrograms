#include<stdio.h>
int main()
{
      double num1, num2, temp;
      printf("Enter first number: ");
      scanf("%lf", &num1);
      printf("Enter second number: ");
      scanf("%lf", &num2);

      // Value of num1 is assigned to temp
      temp = num1;

      // Value of num2 is assigned to num1
      num1 = num2;

      // Value of temp (initial value of num1) is assigned to num2
      num2 = temp;

      printf("\nAfter swapping, first Number = %.2lf", num1);
      printf("\nAfter swapping, second Number = %.2lf", num2);
      return 0;
}
